<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <Button
        android:id="@+id/listmenu_dialog_btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/listmenu_dialog_btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전달 규칙 설정" />

    <Button
        android:id="@+id/listmenu_dialog_btn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전송 이력" />

    <Button
        android:id="@+id/listmenu_dialog_btn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전화번호 수정" />

    <Button
        android:id="@+id/listmenu_dialog_btn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="삭제" />

</LinearLayout>